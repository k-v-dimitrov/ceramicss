{
    "name": "ceramicss",
    "private": true,
    "scripts": {
        "dev": "conc \"yarn next-dev\" \"yarn graphql-codegen:watch\" -n \"next,graphql\" -c \"auto\"",
        "next-dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "prepare": "husky install",
        "format": "prettier --write ./src",
        "storybook": "start-storybook -p 6006 -s ./public",
        "build:storybook": "build-storybook -s public",
        "storybook:build": "build-storybook -s public",
        "graphql-codegen": "graphql-codegen --config codegen.config.yml",
        "graphql-codegen:watch": "graphql-codegen --config codegen.config.yml --watch"
    },
    "dependencies": {
